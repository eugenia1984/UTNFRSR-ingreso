// Prioridad de los operadores aritméticos
Proceso Ejercicio4
	Definir resultado, numero1, numero2 Como Entero;
	Escribir "Ingrese un número: ";  // ingresar el 10
	Leer numero1;
	Escribir "Ingrese otro número: ";  // ingresar el 8
	Leer numero2;
	
	resultado <- numero1 + numero2;
	
	Escribir "El resultado de la suma entre : ",numero1, " y ", numero2, " es: ", resultado;  // El resultado de la suma estre 10 y 8 es: 18
	
FinProceso
